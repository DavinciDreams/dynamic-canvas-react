{
  "name": "@dynamic-canvas/react",
  "version": "0.1.0",
  "type": "module",
  "description": "A modular, reusable dynamic canvas component for React",
  "main": "dist/dynamic-canvas.umd.js",
  "module": "dist/dynamic-canvas.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/dynamic-canvas.es.js",
      "require": "./dist/dynamic-canvas.cjs.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "type-check": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "dependencies": {
    "framer-motion": "^11.0.0",
    "lucide-react": "^0.400.0",
    "zustand": "^5.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.0",
    "typescript": "^5.5.0",
    "vite": "^7.3.1",
    "vite-plugin-dts": "^4.0.0"
  },
  "keywords": [
    "react",
    "canvas",
    "visualization",
    "d3",
    "timeline",
    "chart",
    "component",
    "ui",
    "dynamic"
  ],
  "author": "",
  "license": "MIT"
}
